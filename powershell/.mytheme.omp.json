{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "template": "<#FFFFFF>[‚ùØ_]</>",
          "background" : "#5f5f5f",
          "type": "text",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none",
            "style": "folder"
          },
          "template": " {{ .Path }}",
          "foreground": "#0092FF",
          "type": "path",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none",
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "template": " <#5f5f5f>On</> <white>git(</><#3fff78>{{ .HEAD }}</>{{ if .Working.Changed }} <#FF000F>{{ .Working.String }}</>{{ end }}<white>)</>",
          "type": "git",
          "style": "plain"
        }        
      ]
    }
  ],
  "version": 3,
  "final_space": true
}
